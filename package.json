{
  "name": "fluffy-hopper-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run prebuild && next dev --turbopack",
    "prebuild": "npx ts-node --project strapi/tsconfig.scripts.json -r tsconfig-paths/register strapi/download-remote-images.ts",
    "build": "next build && next-image-export-optimizer",
    "start": "npx serve out",
    "lint": "next lint",
    "format": "prettier --write app components lib data types constants strapi",
    "fix-next-async-request-api": "npx @next/codemod@canary next-async-request-api .",
    "types:generate": "openapi-typescript ./strapi/full_documentation.json -o ./types/strapi.d.ts",
    "clean": "rm -rf .next/cache/fetch-cache"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.501.0",
    "next": "15.2.4",
    "next-image-export-optimizer": "^1.18.0",
    "openapi-fetch": "^0.13.7",
    "openapi-typescript": "^7.7.1",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.2",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^20",
    "@types/qs": "^6.9.18",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.31.1",
    "autoprefixer": "^10.4.21",
    "crypto": "^1.0.1",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.2",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5",
    "typescript-eslint": "^8.31.1"
  }
}
